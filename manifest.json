{
   "background": {
      "service_worker":  "service_worker.js",
      "type": "module"
   },
   "action": {
      "default_icon": "icon128.png",
      "default_popup": "extension.html",
      "default_title": "__MSG_appTitle__"
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [
      "contextMenus",
      "storage"
   ],
   "version": "2.0"
}
